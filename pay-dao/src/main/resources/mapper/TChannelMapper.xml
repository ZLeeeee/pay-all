<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chaotu.pay.dao.TChannelMapper">
  <resultMap id="BaseResultMap" type="com.chaotu.pay.po.TChannel">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="id" jdbcType="BIGINT" property="id" />
    <result column="create_by" jdbcType="VARCHAR" property="createBy" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="update_by" jdbcType="VARCHAR" property="updateBy" />
    <result column="update_time" jdbcType="TIMESTAMP" property="updateTime" />
    <result column="merchant" jdbcType="VARCHAR" property="merchant" />
    <result column="signkey" jdbcType="VARCHAR" property="signkey" />
    <result column="channelName" jdbcType="VARCHAR" property="channelName" />
    <result column="channelCode" jdbcType="VARCHAR" property="channelCode" />
    <result column="refererDomain" jdbcType="VARCHAR" property="refererdomain" />
    <result column="status" jdbcType="CHAR" property="status" />
    <result column="channelQuota" jdbcType="INTEGER" property="channelquota" />
    <result column="tradeAmount" jdbcType="DECIMAL" property="tradeamount" />
    <result column="extend" jdbcType="VARCHAR" property="extend" />
  </resultMap>

  <!--修改通道-->
  <update id="updateChannel" parameterType="com.chaotu.pay.vo.ChannelVo">
    update t_channel
    <trim prefix="set" suffixOverrides=",">
        <if test="merchant!=null and merchant!=''">
            merchant=#{merchant},
        </if>
        <if test="signkey!=null and signkey!=''">
            signkey=#{signkey},
        </if>
        <if test="channelName!=null and channelName!=''">
            channelName=#{channelName},
        </if>
        <if test="channelCode!=null and channelCode!=''">
            channelCode=#{channelCode},
        </if>
        <if test="extend!=null and extend!=''">
            extend=#{extend},
        </if>
        <if test="status!=null and status!=''">
            status=#{status},
        </if>
        <if test="updateTime!=null">
            update_time=#{updateTime,jdbcType=TIMESTAMP},
        </if>
    </trim>
    where id=#{id}
  </update>

</mapper>